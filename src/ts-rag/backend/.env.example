# MongoDB Atlas
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/
DATABASE_NAME=ts_rag

# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# Modelo para chat/geração de texto (escolha um):
# - gemini-2.5-flash (RECOMENDADO - melhor custo/benefício, latest stable)
# - gemini-2.5-pro (mais poderoso, raciocínio complexo)
# - gemini-2.0-flash (geração anterior)
GEMINI_MODEL=gemini-2.5-flash

# Modelo de embeddings (RECOMENDADO: gemini-embedding-001)
# - gemini-embedding-001: Atual, #1 MTEB, 768 dims, suporte 100+ idiomas
# - text-embedding-004: Legacy, será descontinuado em Jan/2026
GEMINI_EMBEDDING_MODEL=gemini-embedding-001

# Server
PORT=3333